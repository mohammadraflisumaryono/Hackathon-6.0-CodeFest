C:/Users/05taq/Documents/blockchain codefest/Hackathon-6.0-CodeFest/#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,/,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/@ethereumjs/rlp/bin/@ethereumjs/rlp/bin/rlp" "$@"
fi
